{
    "name": "the7ofdiamonds/seven-tech-gateway",
    "description": "Wordpress Plugin",
    "type": "project",
    "authors": [
        {
            "name": "Jamel C. Lyons",
            "email": "jclyonsenterprises@gmail.com",
            "homepage": "https://www.linkedin.com/in/jamelclyons/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../seven-tech-communications"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "symfony/dotenv": "^6.3",
        "google/apiclient": "^2.15",
        "kreait/firebase-php": "^7.5",
        "google/apiclient-services": "*",
        "php": "^8.3",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "phpmailer/phpmailer": "^6.8",
        "predis/predis": "^2.2",
        "mkorkmaz/redislabs-rejson": "^2.3",
        "phpoffice/phpspreadsheet": "^3.3",
        "the7ofdiamonds/seven-tech-communications": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "SEVEN_TECH\\Gateway\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-account": "vendor/bin/phpunit ./Test/Account/AccountTest.php",
        "test-account-create": "vendor/bin/phpunit ./Test/Account/CreateTest.php",
        "test-account-details": "vendor/bin/phpunit ./Test/Account/DetailsTest.php",
        "test-admin": "vendor/bin/phpunit ./Test/Admin/AdminTest.php",
        "test-auth": "vendor/bin/phpunit ./Test/Authentication/AuthenticationTest.php",
        "test-change": "vendor/bin/phpunit ./Test/Change/ChangeTest.php",
        "test-database-exists": "vendor/bin/phpunit ./Test/Database/DatabaseExistsTest.php",
        "test-password": "vendor/bin/phpunit ./Test/Password/PasswordTest.php",
        "test-session": "vendor/bin/phpunit ./Test/Session/SessionTest.php",
        "test-session-wordpress": "vendor/bin/phpunit ./Test/Session/SessionWordpressTest.php",
        "test-subscription": "vendor/bin/phpunit ./Test/Subscription/SubscriptionTest.php",
        "test-user-add": "vendor/bin/phpunit ./Test/User/AddTest.php"
    }
}